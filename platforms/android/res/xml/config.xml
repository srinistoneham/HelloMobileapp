<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.hello" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WizSpinnerPlugin">
        <param name="android-package" value="jp.wizcorp.phonegap.plugin.wizSpinner.WizSpinnerPlugin" />
    </feature>
    <feature name="SpinnerPlugin">
        <param name="android-package" value="it.mobimentum.phonegapspinnerplugin.SpinnerPlugin" />
    </feature>
    <feature name="SpinnerDialog">
        <param name="android-package" value="hu.dpal.phonegap.plugins.SpinnerDialog" />
    </feature>
    <name>Srini Testing App</name>
    <description>
        AAGIT Time Tracking Mobile Application
    </description>
    <author email="cpratt@ikmsgroup.com" href="http://www.ikmsgroup.com/">
        AAGIT Team
    </author>
    <preference name="phonegap-version" value="3.6.3" />
    <content src="index.html" />
    <access origin="*" />
    <cdv:plugin name="it.mobimentum.phonegapspinnerplugin" />
</widget>
